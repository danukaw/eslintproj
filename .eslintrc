{
  "extends":"airbnb",
  "parserOptions" : {
      "ecmaVersion" : 6
  },
  "env": {
    "node": true,
    "browser":true,
    "es6":true
  },
  "rules": {
        "comma-dangle" : [2, {"objects": "never", "arrays": "never"}],
        "no-unused-vars":[2, {"vars": "local", "args": "none"}]
    }
}
